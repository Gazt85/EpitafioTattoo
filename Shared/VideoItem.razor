<div class="col-sm-4" height="560">
    <div class="card mb-4" style="width: 18rem;">
        <img src="@ImageSource" class="card-img-top" alt="imagen" height="285">
        <div class="card-body">
            <h5 class="card-title">@Title</h5>
            <h6 class="mb-1 text-muted">@Date.ToString("dd/MM/yyyy")</h6>
            <h6 class="card-text" style="height: 120px;">@Description</h6>
            <a href="@Link" target="_blank" rel="noreferrer" class="btn btn-primary">Ver Video</a>
        </div>
    </div>
    </div>
